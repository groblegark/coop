{
  "allow": [
    "Bash(gh:*)",
    "Bash(oj:*)",
    "Bash(tmux:*)"
  ],
  "hooks": {
    "SessionStart": [
      {
        "matcher": "",
        "hooks": [
          {
            "type": "command",
            "command": "gh help"
          },
          {
            "type": "command",
            "command": "wok help"
          },
          {
            "type": "command",
            "command": "oj help"
          },
          {
            "type": "command",
            "command": "oj run -h"
          }
        ]
      }
    ]
  }
}
